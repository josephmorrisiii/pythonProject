# Press the green button in the gutter to run the script.
if __name__ == '__main__':
    print('Enter integer:')
    usersFirstInteger = int(input())
    print('You entered:', usersFirstInteger)

    print(usersFirstInteger, 'squared is', usersFirstInteger ** 2)
    print('And', usersFirstInteger, 'cubed is', usersFirstInteger ** 3, '!!')

    print('Enter another integer:')
    userSecondInteger = int(input())
    print(usersFirstInteger, '+', userSecondInteger, 'is', usersFirstInteger+userSecondInteger )
    print(usersFirstInteger, '*', userSecondInteger, 'is', usersFirstInteger*userSecondInteger )
